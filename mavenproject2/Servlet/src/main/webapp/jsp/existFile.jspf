<%@page import="java.io.File"%>
<%!
    public String shouRowsTabl() {
        File newsDependeci = new File("c:\\tmp");
        File title[] = newsDependeci.listFiles();
        StringBuilder sb = new StringBuilder();
        for (int i = 0; i < title.length; i++) {
            sb.append("<tr><td>").append(title[i]).append("</td></tr>");
        }
        return sb.toString();
    }
%>
<table border="1">
    <tr>
        <th>Url</th> 
    </tr>
    <%=shouRowsTabl()%>
</table>